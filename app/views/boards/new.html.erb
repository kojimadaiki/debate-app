<%= form_with(model: @board, local: true) do |form| %>

  <div class="title">
    <%= form.label :教える種目 %>
    <%= form.text_field :title %>
  </div>

    <div class="time">
    <%= form.label :時間 %>
    <%= form.datetime_select :start_time %>
  </div>

  <div class="submit">
    <%= form.submit %>
  </div>

  <%= link_to '戻る', boards_path %>

<% end %>